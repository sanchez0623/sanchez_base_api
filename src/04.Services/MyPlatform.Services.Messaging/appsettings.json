{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Port=3306;Database=myplatform_messaging;User=root;Password=yourpassword;AllowPublicKeyRetrieval=True;"
  },
  "Jwt": {
    "SecretKey": "your-jwt-secret-key-at-least-32-characters",
    "Issuer": "MyPlatform",
    "Audience": "MyPlatform.Services"
  },
  "MultiTenancy": {
    "Enabled": true,
    "DefaultTenantId": "default"
  },
  "RabbitMQ": {
    "HostName": "localhost",
    "Port": 5673,
    "UserName": "guest",
    "Password": "guest",
    "VirtualHost": "/",
    "ExchangeName": "myplatform.messaging"
  }
}