{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Port=3306;Database=myplatform_messaging;User=root;Password=yourpassword;AllowPublicKeyRetrieval=True;"
  },
  "Jwt": {
    "SecretKey": "your-jwt-secret-key-at-least-32-characters",
    "Issuer": "MyPlatform",
    "Audience": "MyPlatform.Services"
  },
  "MultiTenancy": {
    "Enabled": true,
    "DefaultTenantId": "default"
  },
  "Redis": {
    "ConnectionString": "localhost:6379",
    "Database": 0,
    "ConnectTimeoutMs": 5000,
    "SyncTimeoutMs": 3000,
    "UseSsl": false
  },
  "RabbitMQ": {
    "HostName": "localhost",
    "Port": 5672,
    "UserName": "guest",
    "Password": "guest",
    "VirtualHost": "/",
    "ExchangeName": "myplatform.events",
    "ExchangeType": "topic",
    "QueueNamePrefix": "myplatform",
    "QueueName": "myplatform.messaging.events",
    "Durable": true,
    "PrefetchCount": 10,
    "RetryCount": 3,
    "RetryDelayMs": 1000
  }
}